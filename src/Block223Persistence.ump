  public void reinitialize() {
  	for( Game game : this.getGames() ) {
  		List<Block> blocks = game.getBlocks();
  	    Block.reinitializeAutouniqueID(blocks); 
  	}
  	Game.reinitializeUniqueGameName(this.getGames());
